{
  "name": "arte-server",
  "version": "1.0.0",
  "description": "An artifact service to store binary data",
  "homepage": "https://github.com/conradoqg/arte-server",
  "repository": {
    "type": "git",
    "url": "https://github.com/conradoqg/arte-server.git"
  },
  "main": "lib/arte-server.js",
  "bin": {
    "arte-server": "bin/arte-server.js"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development node bin//arte-server.js",
    "coveralls": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "coverage": "cross-env NODE_ENV=development LOG_LEVEL=0 nyc --reporter=lcov --reporter=text --clean node node_modules/mocha/bin/_mocha -u bdd --recursive --timeout 60000 --colors test/",
    "test": "cross-env NODE_ENV=development LOG_LEVEL=0 eslint bin lib test && mocha -u bdd --recursive --timeout 60000 --colors test/",
    "lint": "eslint --fix bin lib test"
  },
  "license": "MIT",
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "cfonts": "^1.1.3",
    "chalk": "^2.4.0",
    "compression": "^1.7.2",
    "cookie-parser": "^1.4.3",
    "cross-env": "^5.1.4",
    "easy-rbac": "^3.1.0",
    "es6-error": "^4.1.1",
    "express": "^4.16.3",
    "express-async-errors": "^2.1.2",
    "express-node-metrics": "^1.4.2",
    "express-rate-limit": "^2.11.0",
    "filesize": "^3.6.1",
    "formidable": "^1.2.1",
    "fs-extra": "^5.0.0",
    "helmet": "^3.12.0",
    "jsonwebtoken": "^8.2.1",
    "jstransformer-uglify-js": "^1.2.0",
    "ldapauth-fork": "^4.0.2",
    "moment": "^2.22.1",
    "mongoose": "^5.0.15",
    "password-sheriff": "^1.1.0",
    "pug": "^2.0.3",
    "slug": "^0.9.1",
    "statuses": "^1.5.0",
    "superagent": "^3.8.2",
    "tokenizr": "^1.3.3",
    "yargs": "^11.0.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "eslint": "^4.19.1",
    "js-yaml": "^3.11.0",
    "mocha": "^5.1.1",
    "mocha-steps": "^1.1.0",
    "mockgoose": "^7.3.5",
    "nyc": "^11.7.1",
    "rewire": "^4.0.0",
    "supertest": "git+https://github.com/visionmedia/supertest.git"
  }
